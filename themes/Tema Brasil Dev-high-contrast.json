
{
	"name": "Tema Brasil Dev High Contrast",
	"type": "dark",
	"uiTheme": "hc-black",
	"colors": {
		
		"editor.background": "#000000",
		"editor.foreground": "#FFFFFF",
		"editor.lineHighlightBackground": "#009C3B",
		"editor.selectionBackground": "#FFDF00",
		"editor.inactiveSelectionBackground": "#FFDF0060",
		"editorLineNumber.foreground": "#00FF00",
		
		
		"activityBar.background": "#002200",
		"activityBar.foreground": "#00FF00",
		"activityBarBadge.background": "#FF0000",
		"activityBarBadge.foreground": "#FFFFFF",
		
		"sideBar.background": "#001100",
		"sideBar.foreground": "#FFFFFF",
		"sideBarTitle.foreground": "#FFFF00",
		"sideBarSectionHeader.background": "#009C3B",
		"sideBarSectionHeader.foreground": "#FFFFFF",
		
		
		"statusBar.background": "#FFFF00",
		"statusBar.foreground": "#000000",
		"statusBarItem.hoverBackground": "#FFCC00",
		"statusBar.debuggingBackground": "#FF0000",
		
		"titleBar.activeBackground": "#000000",
		"titleBar.activeForeground": "#FFFFFF",
		"titleBar.inactiveBackground": "#002200",
		"titleBar.inactiveForeground": "#00FF00",
		
		
		"tab.activeBackground": "#000000",
		"tab.activeForeground": "#FFFFFF",
		"tab.inactiveBackground": "#002200",
		"tab.inactiveForeground": "#00FF00",
		"tab.border": "#FFFF00",
		
		
		"button.background": "#009C3B",
		"button.foreground": "#FFFFFF",
		"button.hoverBackground": "#00FF00",
		
		
		"input.background": "#001100",
		"input.foreground": "#FFFFFF",
		"input.border": "#00FF00",
		"input.placeholderForeground": "#00FF00",
		
		
		"list.activeSelectionBackground": "#FF0000",
		"list.focusBackground": "#FFFF00",
		"list.hoverBackground": "#009C3B",
		
		
		"focusBorder": "#FFFF00",
		"selection.background": "#FFFF0060"
	},
	"tokenColors": [
		
		{
			"name": "Comentários",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#00FF00"
			}
		},
		{
			"name": "Strings - Verde Forte",
			"scope": [
				"string",
				"constant.other.symbol",
				"constant.other.key"
			],
			"settings": {
				"foreground": "#00FF00"
			}
		},
		{
			"name": "Números - Amarelo Forte",
			"scope": [
				"constant.numeric",
				"constant.language",
				"keyword.other.unit"
			],
			"settings": {
				"foreground": "#FFFF00"
			}
		},
		{
			"name": "Funções - Azul Claro",
			"scope": [
				"entity.name.function",
				"meta.function-call",
				"support.function"
			],
			"settings": {
				"foreground": "#00FFFF"
			}
		},
		{
			"name": "Classes - Amarelo",
			"scope": [
				"entity.name.class",
				"support.class",
				"support.type"
			],
			"settings": {
				"foreground": "#FFFF00"
			}
		},
		{
			"name": "Keywords - Verde Forte",
			"scope": [
				"keyword",
				"storage.type",
				"storage.modifier"
			],
			"settings": {
				"foreground": "#00FF00"
			}
		},
		{
			"name": "Variáveis - Branco",
			"scope": [
				"variable",
				"meta.block variable.other"
			],
			"settings": {
				"foreground": "#FFFFFF"
			}
		},
		{
			"name": "Operadores - Ciano",
			"scope": [
				"keyword.operator",
				"punctuation"
			],
			"settings": {
				"foreground": "#00FFFF"
			}
		},
		{
			"name": "HTML Tags - Verde",
			"scope": [
				"entity.name.tag",
				"meta.tag.sgml"
			],
			"settings": {
				"foreground": "#00FF00"
			}
		},
		{
			"name": "HTML Attributes - Amarelo",
			"scope": [
				"entity.other.attribute-name"
			],
			"settings": {
				"foreground": "#FFFF00"
			}
		},
		{
			"name": "CSS Properties - Ciano",
			"scope": [
				"support.type.property-name"
			],
			"settings": {
				"foreground": "#00FFFF"
			}
		}
	]
}